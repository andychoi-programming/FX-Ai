{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/venv/**": true,
    "**/__pycache__/**": true,
    "**/*.db": true,
    "**/*.log": true,
    "**/logs/**": true,
    "D:/FX-Ai-logs/**": true
  },
  "search.exclude": {
